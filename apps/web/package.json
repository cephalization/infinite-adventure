{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "remix-serve build",
    "start:prod": "yarn run prisma migrate deploy && yarn start",
    "typecheck": "tsc",
    "lint": "yarn run typecheck && eslint --ext .jsx,tsx,ts,js app/",
    "clean": "rimraf build .cache .turbo"
  },
  "dependencies": {
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@tailwindcss/typography": "^0.5.9",
    "ai-client": "*",
    "clsx": "^1.2.1",
    "daisyui": "^2.50.0",
    "database": "*",
    "env-config": "*",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.1",
    "react-wrap-balancer": "^0.4.0",
    "seamless-scroll-polyfill": "^2.3.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.6",
    "tsconfig": "*",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18"
  }
}
